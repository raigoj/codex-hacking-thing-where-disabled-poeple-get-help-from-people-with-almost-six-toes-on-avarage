

{
  "manifest_version": 2,
  "name": "5.6(6) toes on avg",
  "version": "1.0",
  "description": "Making blind people see",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://volleyball.ee/*",
        "https://kood.tech/*"
      ],

      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "output.png",  
    "48": "output.png",   
    "128": "output.png"  
  }
}

